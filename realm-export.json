{
  "realm": "iprody-lms",
  "enabled": true,
  "clients": [
    {
      "clientId": "basic_client",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "myclient-secret",
      "redirectUris": [
        "http://localhost:8085/*"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true
    }
  ],
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "email": "testuser@example.local",
      "firstName": "Test",
      "lastName": "User",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "testpassword",
          "temporary": false
        }
      ],
      "realmRoles": ["user"]
    },
    {
      "username": "admin",
      "enabled": true,
      "email": "admin@example.local",
      "firstName": "Test",
      "lastName": "User",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "adminpassword",
          "temporary": false
        }
      ],
      "realmRoles": ["admin"]
    },
    {
      "username": "reader",
      "enabled": true,
      "email": "reader@example.local",
      "firstName": "Test",
      "lastName": "User",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "readerpassword",
          "temporary": false
        }
      ],
      "realmRoles": ["reader"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Basic user role"
      },
      {
        "name": "reader",
        "description": "Reader user role"
      },
      {
        "name": "admin",
        "description": "Admin user role"
      }
    ]
  }
